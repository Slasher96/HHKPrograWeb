@model ProyectoPrograWebHHK.Models.DetalleModel
    <div class="table-responsive">
        <table class="table table-striped table-sm">
            <thead>
                <tr>
                    <th>Id Detalle</th>
                    <th>Id Venta</th>
                    <th>SKU</th>
                    <th>Cantidad</th>
                    <th>Costo</th>
                    <th>Total</th>
                 
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model.ListDetalles)
                {
                    <tr>
                        <td>@item.IdDetalle</td>
                        <td>@item.IdVenta</td>
                        <td>@item.Sku</td>
                        <td>@item.Cantidad</td>
                        <td>@item.Costo</td>
                        <td>@item.Total</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
